<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>project0424</display-name>
  <welcome-file-list>
    <welcome-file>main.do</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>mainController</servlet-name>
    <servlet-class>com.webjjang.main.controller.MainController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>mainController</servlet-name>
    <url-pattern>/main/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>boardController</servlet-name>
    <servlet-class>com.webjjang.board.controller.BoardController</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>boardController</servlet-name>
    <url-pattern>/board/*</url-pattern>
  </servlet-mapping>
</web-app>